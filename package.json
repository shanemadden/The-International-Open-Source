{
    "name": "the-international-screeps-bot",
    "version": "2.109.0",
    "description": "",
    "files": [
        "main.js",
        "wasm/pkg/"
    ],
    "main": "dist/main.js",
    "scripts": {
        "build": "rollup -c",
        "build-wasm": "cd wasm && rustup run nightly wasm-pack build --target web --config build.rustflags=['-Ctarget-cpu=mvp'] -Z build-std=std,panic_abort",
        "lint": "eslint src",
        "lint-fix": "eslint src --fix",
        "push-mmo": "rollup -c --environment DEST:mmo",
        "push-pserver": "rollup -c --environment DEST:pserver",
        "watch-mmo": "rollup -cw --environment DEST:mmo",
        "watch-pserver": "rollup -cw --environment DEST:pserver",
        "server": "node server --maxTicks=10000",
        "test": "jest",
        "test-watch": "jest --watch",
        "test-cov": "jest --coverage",
        "test-only": "bash -c 'jest --watch --coverage --verbose --collectCoverageFrom=\"**/*$1*\" $1' -- ",
        "obfuscate": "tsc --target es2019 ./src/international/collectivization/collaborator.ts & javascript-obfuscator ./src/international/collectivization/collaborator.js --self-defending true --dead-code-injection true --control-flow-flattening true",
        "document": "npx typedoc --entryPointStrategy expand ./src"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/The-International-Screeps-Bot/The-International-Open-Source"
    },
    "bugs": {
        "url": "https://github.com/The-International-Screeps-Bot/The-International-Open-Source/issues"
    },
    "author": "",
    "license": "MIT",
    "homepage": "https://github.com/The-International-Screeps-Bot/The-International-Open-Source",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@types/chai": "^4.3.0",
        "@types/jest": "^27.0.1",
        "@types/lodash": "3.10.2",
        "@types/node": "^13.13.1",
        "@types/screeps": "^3.3.0",
        "@types/sinon": "^5.0.5",
        "@types/sinon-chai": "^3.2.0",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "chai": "^4.2.0",
        "dotenv": "^16.0.2",
        "eslint": "^7.23.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsdoc": "^31.6.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-screeps": "^2.1.0",
        "fastestsmallesttextencoderdecoder-encodeinto": "^1.0.22",
        "fs": "^0.0.1-security",
        "javascript-obfuscator": "^4.0.2",
        "jest": "^27.1.0",
        "lodash": "^3.10.1",
        "prettier": "^2.3.2",
        "rollup": "^2.56.2",
        "rollup-plugin-clear": "^2.0.7",
        "rollup-plugin-copy": "^3.4.0",
        "rollup-plugin-screeps": "^1.0.1",
